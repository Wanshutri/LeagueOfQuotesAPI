{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "src": "/", "dest": "/index.js" },
        { "src": "/spanish", "dest": "/index.js" },
        { "src": "/english", "dest": "/index.js" },
        { "src": "/spanish/search", "dest": "/index.js" },
        { "src": "/english/search", "dest": "/index.js" },
        { "src": "/spanish/:id", "dest": "/index.js" },
        { "src": "/english/:id", "dest": "/index.js" }
      ]
}